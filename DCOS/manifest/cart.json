{
    "id": "/robotshop/cart",
    "backoffFactor": 1.15,
    "backoffSeconds": 30,
    "container": {
      "portMappings": [
        {
          "containerPort": 0,
          "labels": {
            "VIP_0": "/robotshop/cart:8080"
          },
          "servicePort": 10008
        }
      ],
      "type": "DOCKER",
      "volumes": [],
      "docker": {
        "image": "robotshop/rs-cart:latest",
        "forcePullImage": false,
        "privileged": false,
        "parameters": []
      }
    },
    "cpus": 0.2,
    "disk": 0,
    "env": {
      "REDIS_HOST": "robotshopredis.marathon.l4lb.thisdcos.directory",
      "CATALOGUE_HOST": "robotshopcatalogue.marathon.l4lb.thisdcos.directory"
    },
    "instances": 1,
    "maxLaunchDelaySeconds": 900,
    "mem": 128,
    "gpus": 0,
    "networks": [
      {
        "name": "dcos",
        "mode": "container"
      }
    ],
    "requirePorts": false,
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 0,
      "expungeAfterSeconds": 0
    },
    "healthChecks": [],
    "fetch": [],
    "constraints": []
  }